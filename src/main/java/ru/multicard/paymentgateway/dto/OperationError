
public enum OperationError {

  NO_ERROR(0, "нет ошибки"),
  XML_REQUEST_ERROR(1, "Ошибка XML запроса"),
  ABSENT_PARAMETER(2,	"Ошибка в переданных параметрах запроса. Отсутствует параметр"),
  MD5_ERROR(3, "Ошибка ключа MD5"),
  4	Ошибка подключения к базе данных
  5	Ошибка выполнения запроса к базе данных
  6	Запрет доступа с неизвестного IP адреса (если требуется проверка)
  10	Платёж с указанной сессией уже имеется в базе данных
  20	Неверный формат номера договора.
    21	Неверный формат сессии
  22	Неверный формат суммы платежа
  23	Неверный формат даты платежа
  30	Не существующий номер договора
  31	Регистрация клиента не была завершена
  32	Расторжение договора
  40	Слишком маленькая сумма платежа
  41	Слишком большая сумма платежа
  50	Платёж на указанный номер договора запрещен


  private final int code;
  private final String text;

  OperationError(int code, String text) {
    this.code = code;
    this.text = text;
  }

  public int getCode() {
    return code;
  }

  public String getText() {
    return text;
  }
}